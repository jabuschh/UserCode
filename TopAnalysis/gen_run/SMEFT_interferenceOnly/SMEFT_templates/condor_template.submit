executable          = run_condor_template.sh
transfer_executable = True
on_exit_hold        = (ExitCode != 0)
universe            = vanilla
output              = condor_logs/condor_$(Cluster)_$(Process).out
error               = condor_logs/condor_$(Cluster)_$(Process).error
log                 = condor_logs/condor_$(Cluster)_$(Process).log
arguments = $(Process)
queue 1300
